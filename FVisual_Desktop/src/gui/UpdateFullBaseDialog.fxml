<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
	<center>
		<TabPane fx:id="tabPaneUpdateTabs" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
			<tabs>
				<Tab fx:id="tabUpdateBase" text="Base" />
				<Tab fx:id="tabUpdateOperationVehicle" text="Operation Vehicles" />
				<Tab fx:id="tabUpdateMember" text="Members" />
			</tabs>
		</TabPane>
	</center>
	<bottom>
		<ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
			<buttons>
				<Button fx:id="btnCancelUpdate" mnemonicParsing="false" onAction="#onClickBtnCancelUpdate" style="-fx-background-color: #BF463E; -fx-text-fill: white;" text="Cancel" />
				<Button fx:id="btnBackUpdate" mnemonicParsing="false" onAction="#onClickBtnBackUpdate" style="-fx-background-color: #BF463E; -fx-text-fill: white;" text="Back" />
				<Button fx:id="btnNextUpdate" mnemonicParsing="false" onAction="#onClickBtnNextUpdate" style="-fx-background-color: #BF463E; -fx-text-fill: white;" text="Next" />
				<Button fx:id="btnSaveUpdate" mnemonicParsing="false" onAction="#onClickBtnSaveUpdate" style="-fx-background-color: #BF463E; -fx-text-fill: white;" text="Save" />
			</buttons>
			<BorderPane.margin>
				<Insets right="5.0" />
			</BorderPane.margin>
		</ButtonBar>
	</bottom>
</BorderPane>
