<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>


<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1">
	<center>
		<GridPane BorderPane.alignment="CENTER">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints maxHeight="-Infinity" minHeight="10.0"
					prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints maxHeight="-Infinity" minHeight="10.0"
					prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<BorderPane.margin>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</BorderPane.margin>
			<children>
				<GridPane GridPane.rowIndex="1">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity"
							minWidth="10.0" prefWidth="100.0" />
						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
							prefWidth="100.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<Label style="-fx-font-weight: bold;" text="Base Name" />
						<Label style="-fx-font-weight: bold;" text="Postcode, Place"
							GridPane.rowIndex="1" />
						<Label style="-fx-font-weight: bold;" text="Address"
							GridPane.rowIndex="2" />
						<Label fx:id="lbBaseName" text="Label" GridPane.columnIndex="1" />
						<Label fx:id="lbPostcodePlace" text="Label"
							GridPane.columnIndex="1" GridPane.rowIndex="1" />
						<Label fx:id="lbAddress" text="Label" GridPane.columnIndex="1"
							GridPane.rowIndex="2" />
					</children>
				</GridPane>
				<Label text="Base ">
					<font>
						<Font name="System Bold Italic" size="16.0" />
					</font>
				</Label>
				<Label text="Operation Vehicles" GridPane.columnIndex="1">
					<GridPane.margin>
						<Insets left="5.0" />
					</GridPane.margin>
					<font>
						<Font name="System Bold Italic" size="16.0" />
					</font>
				</Label>
				<Label text="Members" GridPane.rowIndex="2">
					<font>
						<Font name="System Bold Italic" size="16.0" />
					</font>
				</Label>
				<Label text="Other Organisations" GridPane.columnIndex="1"
					GridPane.rowIndex="2">
					<GridPane.margin>
						<Insets left="5.0" />
					</GridPane.margin>
					<font>
						<Font name="System Bold Italic" size="16.0" />
					</font>
				</Label>
				<ListView fx:id="lvOperationVehicleData" prefHeight="200.0"
					prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
					<GridPane.margin>
						<Insets left="5.0" />
					</GridPane.margin>
				</ListView>
				<ListView fx:id="lvMemberData" prefHeight="200.0"
					prefWidth="200.0" GridPane.rowIndex="3">
					<GridPane.margin>
						<Insets right="5.0" />
					</GridPane.margin>
				</ListView>
				<ListView fx:id="lvOtherOrganisationData" prefHeight="200.0"
					prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
					<GridPane.margin>
						<Insets left="5.0" />
					</GridPane.margin>
				</ListView>
			</children>
		</GridPane>
	</center>
	<bottom>
		<GridPane BorderPane.alignment="CENTER">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<ButtonBar prefHeight="40.0" prefWidth="200.0"
					GridPane.rowIndex="1">
					<buttons>
						<Button fx:id="btnCancel" mnemonicParsing="false"
							onAction="#onClickBtnCancel" text="Cancel" />
						<Button fx:id="btnOk" mnemonicParsing="false" onAction="#onClickBtnOk"
							text="Ok" />
					</buttons>
				</ButtonBar>
				<Label fx:id="lbStatusMessage" style="-fx-font-weight: bold;"
					text="Label" />
			</children>
			<BorderPane.margin>
				<Insets bottom="5.0" left="5.0" right="5.0" />
			</BorderPane.margin>
		</GridPane>
	</bottom>
</BorderPane>
